    "exports": {
        ".": {
            "types": "./dist/vue-json-form.d.ts",
            "import": "./dist/vue-json-form.js",
            "default": "./dist/vue-json-form.js"
        },
        "./dist/*": "./dist/*",
        "./src/exampleSchemas/*": "./src/exampleSchemas/*"
    },
    "files": [
        "dist",
        "src/exampleSchemas",
        "package.json",
        "README.md"
    ],
    "license": "MIT",
    "main": "dist/vue-json-form.cjs",
    "module": "dist/vue-json-form.js",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "turbo build:internal check-types",
        "build:internal": "vite build",
        "check-types": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
        "dev": "vite -c vite.dev.config.ts --host",
        "format": "prettier --write src/",
        "lint": "eslint src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --exit-on-fatal-error",
        "pre-commit": "echo ''",
        "prepack": "yarn run build:internal",
        "prepare": "husky",
        "preview": "vite preview",
        "release:alpha": "yarn npm publish --access public --tag alpha",
        "test": "run-s 'test:*'",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "test:unit": "vitest --run"
    },
    "type": "module",
    "types": "dist/vue-json-form.d.ts"
