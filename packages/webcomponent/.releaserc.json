{
  "branches": [
    {
      "name": "release/latest",
      "channel": "latest"
    },
    {
      "name": "release/2.x",
      "channel": "2.x"
    },
    {
      "name": "release/beta",
      "prerelease": "beta",
      "channel": "beta"
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "semantic-release-yarn",
    [
      "@semantic-release/github",
      {
        "assets": [
          "dist/*.cjs",
          "dist/*.css"
        ]
      }
    ]
  ]
}
