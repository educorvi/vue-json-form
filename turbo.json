{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "check-types": {
      "dependsOn": ["^check-types", "^build"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "doc": {
      "outputs": ["docs-dist"],
      "dependsOn": ["build"]
    },
    "//#doc:root": {
      "dependsOn": ["demo#build", "@educorvi/vue-json-form-schemas#doc"]
    }
  }
}
