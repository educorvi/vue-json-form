{
  "name": "@educorvi/vue-json-form-schemas",
  "version": "0.0.0",
  "description": "Schemas for Vue JSON Form",
  "main": "./dist/vue-json-form-schemas.cjs",
  "module": "./dist/vue-json-form-schemas.js",
  "types": "./dist/vue-json-form-schemas.d.ts",
  "exports": {
    ".": {
      "types": "./dist/vue-json-form-schemas.d.ts",
      "import": "./dist/vue-json-form-schemas.js",
      "require": "./dist/vue-json-form-schemas.cjs",
      "default": "./dist/vue-json-form-schemas.js"
    },
    "./dist/ui.schema.json": "./dist/ui.schema.json",
    "./dist/json-schema_draft7.json": "./dist/json-schema_draft7.json",
    "./package.json": "./package.json"
  },
  "unpkg": "./dist/vue-json-form-schemas.umd.cjs",
  "jsdelivr": "./dist/vue-json-form-schemas.umd.cjs",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "yarn run build",
    "build": "yarn run generateTypings && yarn run mergeSchema && vite build",
    "mergeSchema": "node mergeSchema.cjs",
    "generateTypings": "run-p 'generateTypings:*'",
    "generateTypings:jsonSchema": "json2ts -i src/json-schema_draft7.json -o src/generated/json-schema.d.ts",
    "generateTypings:uiSchema": "cd src/ui/ && json2ts -i ui.schema.json -o ../generated/ui-schema.d.ts"
  },
  "packageManager": "yarn@4.4.0",
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^14.2.1",
    "@educorvi/rita": "^5.4.4"
  },
  "devDependencies": {
    "@semantic-release/exec": "^7.1.0",
    "json-schema-to-typescript": "^15.0.4",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-copy": "^3.5.0",
    "semantic-release": "^25.0.1",
    "semantic-release-monorepo": "^8.0.2",
    "semantic-release-yarn": "^3.0.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.1.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
