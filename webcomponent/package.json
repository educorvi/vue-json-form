{
  "name": "@educorvi/vue-json-form-webcomponent",
  "version": "0.0.0",
  "description": "Vue JSON Form Webcomponent",
  "type": "commonjs",
  "main": "dist/default/vue-json-form.umd.js",
  "module": "dist/default/vue-json-form.mjs",
  "exports": {
    ".": {
      "import": "./dist/default/vue-json-form.mjs",
      "require": "./dist/default/vue-json-form.umd.js"
    },
    "./shadowDom": {
      "import": "./dist/shadowDom/vue-json-form.mjs",
      "require": "./dist/shadowDom/vue-json-form.umd.js"
    },
    "./ajvValidator": {
      "import": "./dist/ajvValidator/vue-json-form.mjs",
      "require": "./dist/ajvValidator/vue-json-form.umd.js"
    },
    "./vue-json-form.css": "./dist/default/vue-json-form.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepack": "yarn workspace vue-json-form run build",
    "build:default": "BUILD_TARGET=default vite build",
    "build:shadowDom": "BUILD_TARGET=shadowDom vite build",
    "build:ajvValidator": "BUILD_TARGET=ajvValidator vite build",
    "build": "run-p 'build:*'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "check-types": "vue-tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/educorvi/vue-json-form.git"
  },
  "dependencies": {
    "@educorvi/vue-json-form": "workspace:^",
    "@educorvi/vue-json-form-ajv-validator": "workspace:^",
    "@vueuse/core": "^14.0.0",
    "axios": "^1.13.1",
    "sass-embedded": "^1.93.2",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@semantic-release/exec": "^7.1.0",
    "@tsconfig/node18": "^18.2.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "npm-run-all": "^4.1.5",
    "semantic-release": "^25.0.1",
    "semantic-release-monorepo": "^8.0.2",
    "semantic-release-yarn": "^3.0.2",
    "terser": "^5.44.0",
    "typescript": "~5.9.3",
    "vite": "^7.1.12",
    "vue-tsc": "^3.1.2"
  }
}
