{
  "name": "@educorvi/vue-json-form-webcomponent",
  "version": "0.0.0",
  "description": "Vue JSON Form Webcomponent",
  "type": "commonjs",
  "main": "dist/vue-json-form.umd.js",
  "module": "dist/vue-json-form.mjs",
  "exports": {
    ".": {
      "import": "./dist/vue-json-form.mjs",
      "require": "./dist/vue-json-form.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepack": "yarn workspace vue-json-form run build:webcomponent",
    "build": "vue-tsc --noEmit && vite build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/educorvi/vue-json-form.git"
  },
  "dependencies": {
    "@educorvi/vue-json-form": "workspace:^",
    "@vueuse/core": "^14.0.0",
    "axios": "^1.13.1",
    "sass-embedded": "^1.93.2",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@semantic-release/exec": "^7.1.0",
    "@tsconfig/node18": "^18.2.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "npm-run-all": "^4.1.5",
    "semantic-release": "^25.0.1",
    "semantic-release-monorepo": "^8.0.2",
    "semantic-release-yarn": "^3.0.2",
    "terser": "^5.44.0",
    "typescript": "~5.9.3",
    "vite": "^7.1.12",
    "vue-tsc": "^3.1.2"
  }
}
